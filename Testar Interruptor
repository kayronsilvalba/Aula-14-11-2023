public class TestarInterruptor{
    public static void main(){
        //Instancie um objeto do tipo interruptor
        Interruptor testarInterruptor = new Interruptor();
        
        testarInterruptor.pressionar();
        testarInterruptor.verificarLampada();
        
        
        testarInterruptor.pressionar();
        testarInterruptor.verificarLampada();
    }
}
